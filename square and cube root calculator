X = float(input("ENTER A POSITIVE REAL NUMBER : ")) 
print(" Type 'a' if you want the value of square root ")
print(" Type 'b' if you want the value of cubic root ")
choose = input("choose(a/b) : ") [X >= 0]
a = X**(1/2)
b = X**(1/3)
if(choose == "a"):
    print("ans : ",a)
elif(choose == "b"):
    print("ans : ",b)
else:
    print("ROOTS OF NEGATIVE REAL NUMBERS ARE IMAGINARY")
